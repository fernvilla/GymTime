<div class="entry">
	<h3>Workout Journal</h3>
	<p class="notice"><%= flash[:notice] %></p>
	<div class="entries">
		<% @entries.each do |entry| %>
			<p>
				<%= link_to entry.title, entry %>
				<%= "(#{entry.entry_date})" %>
			</p>
		<% end %>
	</div>
  <p><%= link_to "Add New Journal Entry", new_entry_path, :class=>"btn btn-primary btn-xs", :id=>"journal" %></p>
  <p><%= link_to "Back to Workouts Calendar", workouts_path %></p>
</div>