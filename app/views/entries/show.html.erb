<div class="entry">
	<h2><%= @entry.title %></h2>
	<h5><%= "(#{@entry.entry_date})" %></h5>
	<hr>
	<h3>Journal Entry </h3>	
	<h5><%= @entry.content %></h5>
	<hr>
	<h3>Workout Log</h3>
	<table id="log">
		<th>Exercise</th>
		<th># Of Sets</th>
		<th># Of Repetitions</th>
		<th>Lbs.</th>
		<tr>
			<td><%= @entry.exercise %></td>
			<td><%= @entry.sets %></td>
			<td><%= @entry.reps %></td>
			<td><%= @entry.lbs %></td>
		</tr>
		<tr>
			<td><%= @entry.exercise_two %></td>
			<td><%= @entry.sets_two %></td>
			<td><%= @entry.reps_two %></td>
			<td><%= @entry.lbs_two %></td>
		</tr>
		<tr>
			<td><%= @entry.exercise_three %></td>
			<td><%= @entry.sets_three %></td>
			<td><%= @entry.reps_three %></td>
			<td><%= @entry.lbs_three %></td>
		</tr>
	</table>
	<br>
	<br>
	<p class="btn btn-success"><%= link_to "Edit Entry", edit_entry_path%></p>
	<p class="btn btn-default"><%= link_to "Go Back", entries_path%></p>
</div>
