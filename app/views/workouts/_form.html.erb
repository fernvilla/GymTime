<%= form_for @workout do |f| %>
	<div>
		<%= f.label "Body Part(s)" %><br />
		<%= f.text_field :body_part, :required => true  %>
	</div>
	<br>
	<div>
			<%= f.label :workout_date%> <br />
			<%= f.text_field :workout_date, :class=>"datepicker", :required => true %>
	</div>
	<div class="exercises">
	<fieldset>
	  <p>Select Exercise(s):</p>
	  <!-- current_user should be added before exercises.all to grab only current users' exercises -->
	  <p class="options"><%= f.collection_check_boxes :exercise_ids, current_user.exercises.all, :id, :name %></p>
	</fieldset>
	</div>
	<div><%= f.submit :class=>"btn btn-success" %></div>
<% end %>