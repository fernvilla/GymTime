<style>
  html { 
    height: 100%;
  }

  body { 
    height: 100%; 
    margin: 0; 
    padding: 0; 
  }  

  #map-canvas { 
    height: 80%; 
    width: 80%;
    margin: 0 auto;
  }
</style>

<script>
  var map;
  function initialize() {
  var mapOptions = {
    zoom: 8, center: new google.maps.LatLng(-34.397, 150.644)
  };

  map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);
  } 
  google.maps.event.addDomListener(window, 'load', initialize);
</script>
<br>
<div id="map-canvas" />
